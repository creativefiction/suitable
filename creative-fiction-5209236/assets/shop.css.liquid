p.popup_product_color {
	margin-top:10px;
}

p.popup_helper {
	margin-bottom:9px;
}

input.popup_button:hover {
	background:rgb(77,71,71);
	border: 1px solid black;
}

.cart_header {
	text-align:center;
	background: black;
	color:white;
	font-size: 18px;
	padding:10px 0;
}

.header_container {
	border-bottom:1px solid black;
}

#cart {
	color:black;
	margin-bottom: 60px;
}

.cart_helper {
	text-align:center;
}

.cart_table_header {
	text-align: left;
	font-style: normal;
	padding-bottom: 7px;
	font-family: 'Quicksand', sans-serif;
	font-size: 16px;
}

.cart_description {
	font-size:18px;
}

.center {
	text-align:center;
}

.minus_counter, .plus_counter {
	padding:2px 0;
	background:rgb(152, 145, 139);
	color:white;
	font-size:20px;
	width: 27px;
	display: inline-block;
	cursor:pointer;
}

.cart_item_quantity {
	margin: 0 10px;
	font-size:18px;
}

.cart_item_properties {
	list-style-type: none;
	width:290px;
}

.cart_item_properties li:nth-of-type(2n+1) { float: left; width:134px; }
.cart_item_properties li:nth-of-type(2n) { float: right; width:146px; }

.order_number {
	width:80%;
}

.note, .date, .order_number, .number {
	font-size: 16px;
	color:black;
	font-family: 'Montserrat', sans-serif;
}

.number {
	margin-left: 57px;
}

.order_summary tr td {
	font-size: 16px;
	font-weight: 400;
	font-family: montserrat;
}

.order_summary tr td:first-child {
	width:247px;
}

.order_summary tr td:last-child {
	padding-right: 0px;
	float: right;
	text-align: right;
}

.order_description_container {
	margin-bottom: 15px;
	height: 563px;
}

.order_description_title {
	border-top: 1px solid hsl(0, 0%, 0%);
	padding-top: 10px;
	display: block;
}

.account_total {
	border-top:1px solid black;
	border-bottom: 1px solid black;
}

.order_measurements {
	font-size:16px;
}

.order_left_measurements {
	float: left;
	width: 190px;
}

.order_right_measurements {
	float: left;
	width: 195px;
	margin-left: 26px;
}

.order_measurements_container {
	padding:10px 0;
}

#customer_orders {
	margin-bottom:60px;
}

.order_x {
	text-align:center;
}

.order_radio {
	margin-right:10px;
}

.video_thumb_container {
	position:relative;
}

.video_play {
	position:absolute;
	top: 13px;
}

.iframe_container {
	position: absolute;
	top: 3px;
}

.product_container_video {
	position:relative;
}

.account_address_shipping {
	float: left;
	margin-left: 150px;
}

.account_address_billing {
	float: left;
	color:black;
}

#view_address {
	font-style: italic;
}

.error_text {
	color: hsl(344, 89%, 31%);
	font-size: 16px;
	margin-left: -347px;
	margin-top: 23px;
	font-weight: 500;
	position: absolute;
}

